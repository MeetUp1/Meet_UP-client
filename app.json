{
  "expo": {
    "name": "MeetUp-client",
    "slug": "MeetUp-client",
    "scheme": "meetup",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.meetup.login"
    },
    "android": {
      "useNextNotificationsApi": true,
      "package": "com.meetup.login",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
